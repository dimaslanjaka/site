{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "cd packages/hexo-admin && npm install && npm run build",
    "start": "npm run server",
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server -p 4000",
    "serve-static": "php -S localhost:4000 -t public/",
    "update": "npx npm-check-updates -u -x chalk && npm install",
    "postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js && node postinstall.js"
  },
  "hexo": {
    "version": "6.3.0"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "hexo": "^6.3.0",
    "hexo-admin": "file:packages/hexo-admin",
    "hexo-browsersync": "https://github.com/hexojs/hexo-browsersync/tarball/master",
    "hexo-generator-archive": "https://github.com/hexojs/hexo-generator-archive/tarball/master",
    "hexo-generator-category": "https://github.com/hexojs/hexo-generator-category/tarball/master",
    "hexo-generator-index": "https://github.com/hexojs/hexo-generator-index/tarball/master",
    "hexo-generator-tag": "https://github.com/hexojs/hexo-generator-tag/tarball/master",
    "hexo-renderer-markdown-it": "https://github.com/hexojs/hexo-renderer-markdown-it/tarball/master",
    "hexo-renderer-pug": "https://github.com/hexojs/hexo-renderer-pug/tarball/master",
    "hexo-renderer-stylus": "https://github.com/hexojs/hexo-renderer-stylus/tarball/master",
    "hexo-server": "^3.0.0",
    "hexo-theme-butterfly": "file:themes/butterfly"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "eslint": "^8.30.0",
    "fs-extra": "^11.1.0",
    "js-yaml": "^4.1.0",
    "moment": "^2.29.4",
    "prettier": "^2.8.1",
    "prompt": "^1.3.0",
    "upath": "^2.0.1",
    "yaml": "^2.1.3"
  }
}