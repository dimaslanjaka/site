{
  "name": "hexo-seo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run server",
    "build": "clear && hexo generate",
    "clean": "clear && hexo clean",
    "deploy": "hexo deploy",
    "server": "clear && hexo server -p 4000",
    "serve-static": "php -S localhost:4000 -t public/",
    "install-git": "npm i git+https://github.com/dimaslanjaka/hexo-seo.git && npm i git+https://github.com/dimaslanjaka/hexo-blogger-xml.git && npm i git+https://github.com/dimaslanjaka/hexo-adsense.git",
    "update": "npx npm-check-updates -u",
    "generate:search": "hexo seo-search",
    "generate:feed": "hexo seo-feed"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "hexo": "^7.3.0",
    "hexo-browsersync": "github:hexojs/hexo-browsersync",
    "hexo-generator-archive": "github:hexojs/hexo-generator-archive",
    "hexo-generator-category": "github:hexojs/hexo-generator-category",
    "hexo-generator-index": "github:hexojs/hexo-generator-index",
    "hexo-generator-tag": "github:hexojs/hexo-generator-tag",
    "hexo-renderer-markdown-it": "github:hexojs/hexo-renderer-markdown-it",
    "hexo-renderer-pug": "github:hexojs/hexo-renderer-pug",
    "hexo-renderer-stylus": "github:hexojs/hexo-renderer-stylus",
    "hexo-seo": "*",
    "hexo-server": "^3.0.0",
    "hexo-shortcodes": "^1.2.4",
    "hexo-util": "^3.3.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "js-yaml": "^4.1.0",
    "moment": "^2.30.1",
    "node-sass": "^9.0.0",
    "prompt": "^1.3.0",
    "yaml": "^2.6.0"
  }
}
