{
  "name": "hexo-seo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run server",
    "build": "clear && hexo generate",
    "clean": "clear && hexo clean",
    "deploy": "hexo deploy",
    "server": "clear && hexo server -p 4000",
    "serve-static": "php -S localhost:4000 -t public/",
    "install-git": "npm i git+https://github.com/dimaslanjaka/hexo-seo.git && npm i git+https://github.com/dimaslanjaka/hexo-blogger-xml.git && npm i git+https://github.com/dimaslanjaka/hexo-adsense.git",
    "update": "npx npm-check-updates -u",
    "generate:search": "hexo seo-search",
    "generate:feed": "hexo seo-feed"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "hexo": "^7.3.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-markdown-it": "^7.1.1",
    "hexo-renderer-pug": "^3.0.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-seo": "*",
    "hexo-server": "^3.0.0",
    "hexo-shortcodes": "^1.2.4",
    "hexo-util": "^3.3.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "js-yaml": "^4.1.0",
    "moment": "^2.30.1",
    "prompt": "^1.3.0",
    "yaml": "^2.8.0"
  }
}
