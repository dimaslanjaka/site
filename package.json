{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run server",
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server -p 4000",
    "serve-static": "php -S localhost:4000 -t public/",
    "update": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js && npx npm-check-updates -u -x chalk,hexo,codemirror && npm install",
    "postinstall": "node postinstall.js",
    "test": "cd .. && npm test"
  },
  "hexo": {
    "version": "6.3.0"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "hexo": "^6.3.0",
    "hexo-browsersync": "git+https://github.com/hexojs/hexo-browsersync",
    "hexo-generator-archive": "git+https://github.com/hexojs/hexo-generator-archive",
    "hexo-generator-category": "git+https://github.com/hexojs/hexo-generator-category",
    "hexo-generator-index": "git+https://github.com/hexojs/hexo-generator-index",
    "hexo-generator-tag": "git+https://github.com/hexojs/hexo-generator-tag",
    "hexo-renderer-markdown-it": "git+https://github.com/hexojs/hexo-renderer-markdown-it",
    "hexo-renderer-pug": "git+https://github.com/hexojs/hexo-renderer-pug",
    "hexo-renderer-stylus": "git+https://github.com/hexojs/hexo-renderer-stylus",
    "hexo-server": "^3.0.0",
    "hexo-theme-butterfly": "file:themes/butterfly",
    "hexo-util": "^3.0.1"
  },
  "devDependencies": {
    "axios": "^1.2.3",
    "axios-cache-interceptor": "^1.0.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/tarball/private",
    "hpagent": "^1.2.0",
    "js-yaml": "^4.1.0",
    "moment": "^2.29.4",
    "prompt": "^1.3.0",
    "static-blog-generator": "file:../release/static-blog-generator.tgz",
    "yaml": "^2.2.1"
  }
}
